<%= form_with(model: custom_travel) do |form| %>
  <% if custom_travel.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(custom_travel.errors.count, "error") %> prohibited this custom_travel from being saved:</h2>

      <ul>
        <% custom_travel.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :departure, style: "display: block" %>
    <%= form.text_field :departure %>
  </div>

  <div>
    <%= form.label :destination, style: "display: block" %>
    <%= form.text_field :destination %>
  </div>

  <div>
    <%= form.label :date, style: "display: block" %>
    <%= form.date_field :date %>
  </div>

  <div>
    <%= form.label :price, style: "display: block" %>
    <%= form.text_field :price %>
  </div>

  <div>
    <%= form.label :rating, style: "display: block" %>
    <%= form.text_field :rating %>
  </div>

  <div>
    <%= form.label :comments, style: "display: block" %>
    <%= form.text_area :comments %>
  </div>

  <div>
    <%= form.label :physical, style: "display: block" %>
    <%= form.text_field :physical %>
  </div>

  <div>
    <%= form.label :characteristics, style: "display: block" %>
    <%= form.text_field :characteristics %>
  </div>

  <div>
    <%= form.label :event_bool, style: "display: block" %>
    <%= form.check_box :event_bool %>
  </div>

  <div>
    <%= form.label :event, style: "display: block" %>
    <%= form.text_area :event %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
